View(pred_data)
library(dplyr)
pr_df <- pred_data %>%
arrange(desc(predicted_value_1)) %>%
mutate(
tp = cumsum(true_label == 1),
fp = cumsum(true_label == 0),
fn = sum(true_label == 1) - tp,
precision = tp / (tp + fp),
recall = tp / (tp + fn)
)
ggplot(pr_df, aes(x = recall, y = precision)) +
geom_line(color = "darkred") +
theme_minimal()
