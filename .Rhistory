ylim(0, 1) + xlim(0, 1) +
labs(
x = "Recall (TPR)",
y = "Precision (PPV)",
title = paste0("Precision–Recall Curve (AUC = ", round(pr_auc, 3), ")")
) +
theme_minimal()
